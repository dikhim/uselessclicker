$btnTemplateSystemPrint:
system.print('');
//Вывести сообщение в поле вывода
//
//system.print('TEXT');

$btnTemplateSystemPrintCode:
system.print('');

$btnTemplateSystemPrintln:
system.println('');
//Вывести сообщение в поле вывода
//и перейти на новую строку
//
//system.println('TEXT');

$btnTemplateSystemPrintlnCode:
system.println('');

$btnTemplateSystemRegisterShortcut:
