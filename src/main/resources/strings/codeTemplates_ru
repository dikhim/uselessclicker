}

$btnTemplateLanguageLoopForPlus:
for(i=0;i<10;i++){

}
//Шаблон возрастающего цикла 'for'

$btnTemplateLanguageLoopForPlusCode:
for(i=0;i<10;i++){

}

$btnTemplateLanguageLoopForMinus:
for(i=10;i>0;i--){

}
Шаблон убывающего цикла 'for'

$btnTemplateLanguageLoopForMinusCode:
for(i=10;i>0;i--){

}

$btnTemplateLanguageLoopWhile:
