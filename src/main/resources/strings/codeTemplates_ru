$btnTemplateLanguageClass:
function Class (name) {
  this.name = name;
  this.echo = function(text){
    return text;
  };
}
//Шаблон класса с именем 'Class'
//
//Для создания объекта используйте
//конструкцию типа
//var obj = new Class('MyClass');
//
//Обращение к полю объекта:
//var out = obj.name;
//
//Обращение к методу объекта:
//var out = obj.echo('someText');

$btnTemplateLanguageClassCode:
function Class (name) {
  this.name = name;
  this.echo = function(text){
    return text;
  };
}

$btnTemplateLanguageFunctionEcho:
function echo(text){
  return text;
}
//Шаблон функции сполучением и
//возвратом значения

$btnTemplateLanguageFunctionEchoCode:
function echo(text){

  return text;
}

