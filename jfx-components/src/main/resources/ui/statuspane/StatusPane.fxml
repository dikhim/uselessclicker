        <ToggleButton id="lupeStatusButton" fx:id="btnLupeStatus" mnemonicParsing="false" styleClass="statusButton" HBox.hgrow="NEVER" />
        <ToggleButton id="mouseStatusButton" fx:id="btnMouseRecordingStatus" disable="true" mnemonicParsing="false" styleClass="statusButton" HBox.hgrow="NEVER" />
        <ToggleButton id="keyboardStatusButton" fx:id="btnKeyboardRecordingStatus" disable="true" mnemonicParsing="false" styleClass="statusButton" HBox.hgrow="NEVER" />
        <ToggleButton id="recordingStatusButton" fx:id="btnRecordingStatus" disable="true" mnemonicParsing="false" styleClass="statusButton" HBox.hgrow="NEVER" />
