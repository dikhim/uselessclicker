    <children>
        <ToggleButton fx:id="btnScriptStatus" maxWidth="250.0" minWidth="250.0" mnemonicParsing="false" onAction="#scriptStatus" prefWidth="250.0" styleClass="templateButton" text="Running:   newFile.js" HBox.hgrow="NEVER" />
